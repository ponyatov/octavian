<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="octavian.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="octavian.Perspective"
            id="octavian.perspective"
            name="Octavian"
            icon="icons/octave.png"
            >
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
         <view
               class="octavian.Nodes"
               id="octavian.nodes"
               icon="icons/sample.gif"
               name="Узлы"
               >
         </view>
   </extension>

</plugin>
